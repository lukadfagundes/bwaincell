{
  "name": "@bwaincell/backend",
  "version": "1.0.0",
  "description": "Bwaincell Discord Bot Backend",
  "main": "dist/src/bot.js",
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@commands": "dist/commands",
    "@utils": "dist/utils",
    "@database": "dist/database",
    "@config": "dist/config",
    "@types": "dist/types",
    "@src": "dist/src"
  },
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e ../.env -- ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/bot.ts",
    "start": "node dist/src/bot.js",
    "deploy": "dotenv -e ../.env -- node dist/src/deploy-commands.js",
    "generate-assets": "ts-node generate-assets.ts",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage-report": "jest --coverage --coverageReporters=html text",
    "coverage:threshold": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "lint": "eslint \"src/**/*.ts\" \"commands/**/*.ts\" \"database/**/*.ts\" \"utils/**/*.ts\" \"shared/**/*.ts\" \"types/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"commands/**/*.ts\" \"database/**/*.ts\" \"utils/**/*.ts\" \"shared/**/*.ts\" \"types/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "setup": "npm run build && npm run deploy",
    "start:fresh": "npm run setup && npm start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@napi-rs/canvas": "^0.1.80",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/luxon": "^3.7.1",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "google-auth-library": "^10.4.0",
    "googleapis": "^160.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "module-alias": "^2.2.3",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "trinity-method-sdk": "^2.0.8",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@discordjs/builders": "^1.11.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@typescript-eslint/utils": "^8.44.1",
    "c8": "^10.1.3",
    "coverage-badge-creator": "^1.0.21",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.3",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,tsx,json,md}": [
      "prettier --write"
    ]
  }
}
