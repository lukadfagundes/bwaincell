{
  "name": "bwaincell-monorepo",
  "version": "2.0.0",
  "private": true,
  "description": "Bwaincell Discord Bot + PWA Monorepo",
  "workspaces": [
    "backend",
    "frontend",
    "shared"
  ],
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@commands": "dist/commands",
    "@utils": "dist/utils",
    "@database": "dist/database",
    "@config": "dist/config",
    "@types": "dist/types",
    "@src": "dist/src"
  },
  "scripts": {
    "dev": "npm run build:shared && concurrently -n backend,frontend -c blue,magenta \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:shared": "npm run watch --workspace=shared",
    "build": "npm run build --workspace=shared && npm run build --workspaces --if-present",
    "build:shared": "npm run build --workspace=shared",
    "build:backend": "npm run build:shared && npm run build --workspace=backend",
    "build:frontend": "npm run build:shared && npm run build --workspace=frontend",
    "build:all": "tsc --build",
    "test": "npm run test --workspaces --if-present",
    "test:backend": "npm run test --workspace=backend",
    "test:frontend": "npm run test --workspace=frontend",
    "test:shared": "npm run test --workspace=shared --if-present",
    "test:watch": "npm run test:watch --workspaces --if-present",
    "test:coverage": "npm run test:coverage --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "lint:backend": "npm run lint --workspace=backend",
    "lint:frontend": "npm run lint --workspace=frontend",
    "typecheck": "tsc --build",
    "typecheck:watch": "tsc --build --watch",
    "clean": "npm run clean --workspaces --if-present && tsc --build --clean",
    "clean:backend": "npm run clean --workspace=backend",
    "clean:frontend": "npm run clean --workspace=frontend",
    "clean:shared": "npm run clean --workspace=shared",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:backend": "docker compose logs -f backend",
    "docker:frontend": "docker compose logs -f frontend",
    "postinstall": "test \"$VERCEL\" = \"1\" && echo 'Skipping postinstall on Vercel' || npm run build:shared --if-present",
    "prepare": "test \"$VERCEL\" = \"1\" && echo 'Skipping prepare on Vercel' || husky || true"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@google/genai": "^1.40.0",
    "@napi-rs/canvas": "^0.1.80",
    "@octokit/rest": "^20.1.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/luxon": "^3.7.1",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "google-auth-library": "^10.4.0",
    "googleapis": "^160.0.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "module-alias": "^2.2.3",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "trinity-method-sdk": "^2.0.8",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@discordjs/builders": "^1.11.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@typescript-eslint/utils": "^8.44.1",
    "c8": "^10.1.3",
    "concurrently": "^9.1.2",
    "coverage-badge-creator": "^1.0.21",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.3",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.0.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "backend/**/*.ts": [
      "bash -c 'cd backend && eslint --fix \"${0#backend/}\"'",
      "prettier --write"
    ],
    "frontend/**/*.{ts,tsx}": [
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
