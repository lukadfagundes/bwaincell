{
  "name": "bwaincell",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/bot.js",
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@commands": "dist/commands",
    "@utils": "dist/utils",
    "@database": "dist/database",
    "@config": "dist/config",
    "@types": "dist/types",
    "@src": "dist/src"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/bot.ts",
    "start": "node dist/src/bot.js",
    "deploy": "node dist/src/deploy-commands.js",
    "generate-assets": "ts-node generate-assets.ts",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage-report": "jest --coverage --coverageReporters=html text",
    "coverage:threshold": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "setup": "npm run build && npm run deploy",
    "start:fresh": "npm run setup && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@napi-rs/canvas": "^0.1.80",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.2",
    "googleapis": "^160.0.0",
    "joi": "^18.0.1",
    "module-alias": "^2.2.3",
    "node-cron": "^4.2.1",
    "sequelize": "^6.37.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@discordjs/builders": "^1.11.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@jest/globals": "^30.1.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@typescript-eslint/utils": "^8.44.1",
    "c8": "^10.1.3",
    "coverage-badge-creator": "^1.0.21",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "glob": "^11.0.3",
    "jest": "^30.1.3",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
